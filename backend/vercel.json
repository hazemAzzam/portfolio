{
  "version": 2,
  "builds": [
    {
      "src": "backend/wsgi.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "backend/wsgi.py"
    },
    {
      "src": "/admin/(.*)",
      "dest": "backend/wsgi.py"
    },
    {
      "src": "/static/(.*)",
      "dest": "backend/wsgi.py"
    }
  ],
  "env": {
    "DEBUG": "False",
    "SECRET_KEY": "@secret-key",
    "ALLOWED_HOSTS": "dashboard-seven-jade.vercel.app,portfolio-three-fawn-30.vercel.app"
  }
}